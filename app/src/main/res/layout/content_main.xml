<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.niugulu.testproject.MainActivity"
    tools:showIn="@layout/activity_main">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_create_observable"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="Create Observable" />

        <TextView
            android:id="@+id/tv_filter_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="Filter Method" />

        <TextView
            android:id="@+id/tv_take_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="Take Method" />

        <TextView
            android:id="@+id/tv_take_last_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="TakeLast Method" />

        <TextView
            android:id="@+id/tv_distinct_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="Distinct Method" />

        <TextView
            android:id="@+id/tv_first_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="First Method" />

        <TextView
            android:id="@+id/tv_skip_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="Skip Method" />


        <TextView
            android:id="@+id/tv_element_at_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="ElementAt Method" />

        <TextView
            android:id="@+id/tv_sampling_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="Sampling Method" />

        <TextView
            android:id="@+id/tv_timeout_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="Timeout Method" />

        <TextView
            android:id="@+id/tv_debounce_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="Debounce Method" />

        <TextView
            android:id="@+id/tv_map_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="Map Method" />

        <TextView
            android:id="@+id/tv_flat_map_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="Flat Map Method" />

        <TextView
            android:id="@+id/tv_flat_map_interable_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="Flat Map Iterable Method" />

        <TextView
            android:id="@+id/tv_scan_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="Scan Method" />

        <TextView
            android:id="@+id/tv_group_by_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="Group By Method" />

        <TextView
            android:id="@+id/tv_buffer_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="Buffer Method" />

        <TextView
            android:id="@+id/tv_window_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="Window Method" />

        <TextView
            android:id="@+id/tv_cast_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="Cast Method" />

        <TextView
            android:id="@+id/tv_merge_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="Merge Method" />

        <TextView
            android:id="@+id/tv_zip_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="Zip Method" />

        <TextView
            android:id="@+id/tv_join_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="Zip Method" />

        <TextView
            android:id="@+id/tv_combine_latest_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="combineLatest Method" />

        <TextView
            android:id="@+id/tv_switch_method"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:background="#00ba9b"
            android:gravity="center"
            android:text="Switch Method" />
    </LinearLayout>
</ScrollView>
